{
  \"name\": \"$$TARGET\",
  \"version\": \"$$HA_VERSION\",
  \"build\": {
    \"date\": \"$$BUILDDATE\",
    \"qt\": \"$$QT_VERSION\",
    \"debug\": $$DEBUG_BUILD,
    \"git\": {
        \"hash\": \"$$GIT_HASH\",
        \"branch\": \"$$GIT_BRANCH\"
    },
    \"host\": {
        \"arch\": \"$$QMAKE_HOST.arch\",
        \"os\": \"$$QMAKE_HOST.os\",
        \"name\": \"$$QMAKE_HOST.name\",
        \"version_string\": \"$$QMAKE_HOST.version_string\"
    }
  },
  \"copyright\" : \"(C) 2019-2020 Contributors of integration.homeassistant\",
  \"license\" : [
      \"YIO-Remote software is free software: you can redistribute it and/or modify\",
      \"it under the terms of the GNU General Public License as published by\",
      \"the Free Software Foundation, either version 3 of the License, or\",
      \"(at your option) any later version.\",
      \"SPDX-License-Identifier: GPL-3.0-or-later\"
  ],
  \"category\" : \"Integrations\",
  \"description\":  [
      \"Home Assistant Integration for YIO\"
  ],
  \"url\": \"https://github.com/YIO-Remote/integration.home-assistant\",
  \"dependencies\" : [
      { \"name\" : \"app\", \"version\" : \"0.2.0\" },
      { \"name\" : \"remoteOS\", \"version\" : \"0.2.0\" }
  ],
  \"setup_data\": {
    \"$schema\": \"http://json-schema.org\",
    \"$id\": \"http://yio-remote.com/homeassistant.json\",
    \"type\": \"object\",
    \"title\": \"YIO Integration Home Assistant Schema\",
    \"description\": \"Required data points to set up a Home Assistant integrations.\",
    \"default\": {},
    \"additionalProperties\": true,
    \"examples\": [
        {
            \"token\": \"\",
            \"ip\": \"\"
        }
    ],
    \"required\": [
        \"ip\"
    ],
    \"properties\": {
        \"ip\": {
            \"$id\": \"#/properties/ip\",
            \"type\": \"string\",
            \"title\": \"The ip schema\",
            \"description\": \"The IP address and port of your Home Assistant server.\",
            \"default\": \"\",
            \"examples\": [
                \"192.168.100.2:8123\"
            ]
        },
        \"token\": {
            \"$id\": \"#/properties/token\",
            \"type\": \"string\",
            \"title\": \"The token schema\",
            \"description\": \"Long lived token generated from Home Assistant.\",
            \"default\": \"\",
            \"examples\": [
                \"eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJ1c2VyIjoiVlx1MDAxNcKbwoNUwoonbFPCu8KhwrYiLCJpYXQiOjE0NDQyNjI1NDMsImV4cCI6MTQ0NDI2MjU2M30.MldruS1PvZaRZIJR4legQaauQ3_DYKxxP2rFnD37Ip4\"
            ]
        }
    }
  }
}
